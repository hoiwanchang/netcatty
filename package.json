{
  "name": "nebula-ssh",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"npm:dev:electron\"",
    "dev:electron": "wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 node electron/launch.cjs",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node electron/launch.cjs",
    "postinstall": "npx @electron/rebuild",
    "rebuild": "npx @electron/rebuild"
  },
  "dependencies": {
    "@google/genai": "1.31.0",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-tabs": "1.1.13",
    "clsx": "2.1.0",
    "ghostty-web": "^0.3.0",
    "lucide-react": "0.556.0",
    "node-pty": "^1.0.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "ssh2-sftp-client": "^9.1.0",
    "tailwind-merge": "2.2.0",
    "uuid": "^9.0.1",
