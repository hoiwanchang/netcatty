    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^39.2.6",
    "eslint": "^9.39.1",
